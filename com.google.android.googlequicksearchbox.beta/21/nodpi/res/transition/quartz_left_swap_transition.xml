<?xml version="1.0" encoding="utf-8"?>
<transitionSet android:transitionOrdering="together"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <transition android:interpolator="@interpolator/quartz_0_0_0_1_bezier" android:duration="500" app:directionMode="direction_decrease" app:dimension="y" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.TranslationTransition">
        <targets>
            <target android:targetName="@string/quartz_canvas_left_transition_name" />
        </targets>
    </transition>
    <transition android:interpolator="@interpolator/quartz_0_0_1_1_bezier" android:duration="200" app:fadingMode="fade_in" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.AlphaTransition">
        <targets>
            <target android:targetName="@string/quartz_canvas_left_transition_name" />
        </targets>
    </transition>
    <transition android:duration="100" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.BackgroundTransition">
        <targets>
            <target android:targetName="@string/quartz_canvas_background_transition_name" />
        </targets>
    </transition>
</transitionSet>