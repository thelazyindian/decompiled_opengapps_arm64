<?xml version="1.0" encoding="utf-8"?>
<framework>
    <module class="com.google.android.apps.inputmethod.libs.search.sticker.BitmojiExtension">
        <initialization_deps>
            <system_property value="@string/system_property_enable_bitmoji" />
        </initialization_deps>
        <keyboard_group>
            <keyboard type="prime" def="@xml/keyboard_bitmoji_search" />
        </keyboard_group>
        <keyboard_types>
            <keyboard type="@string/keyboard_type_bitmoji_search_result" />
        </keyboard_types>
        <extra_values>
            <item id="@id/extra_value_default_keyboard" value="@string/keyboard_type_bitmoji_search_result" />
        </extra_values>
    </module>
</framework>